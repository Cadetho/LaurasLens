<div class="gallery_wrap">
	<div class="slider-wrap">
		<h1>Categories</h1>
		<div class="slider">
		{% for p in page.find('/gallery').children if p != page %}
			<div class="gallery-tile">
				<div class="category-image">
					<img src="../LaurasLens/gallery/{{p.title}}/{{p.header.mainpic}}">
				</div>
				<div class="category-info">
				<h2>{{p.header.categoryName}}</h2>
				<h3>{{p.header.Date}}</h3>
				</div>
			</div>
		{% endfor %}
		</div>
	</div>
</div>